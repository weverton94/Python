      "source": [
        "def ehPar(n):\n",
        "  r = (n%2==0)\n",
        "  return r\n",
        "\n",
        "def somar_par(lst):\n",
        "   soma=0\n",
        "   for num in lst:\n",
        "    if (ehPar(num)):\n",
        "      soma=soma+num\n",
        "   return soma\n",
        "\n",
        "lista = [10,2,5,7,6,3]\n",
        "soma=somar_par(lista)\n",
        "print(f\"O somatório dos elementos pares da lista é: {soma}\")"
      ]
    }
